{
  "name": "simpod-sankey-panel",
  "description": "Grafana Sankey Diagram Panel",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier:check": "prettier --list-different \"**/*.{ts,tsx,scss}\"",
    "prettier:write": "prettier --list-different \"**/*.{ts,tsx,scss}\" --write",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simPod/grafana-sankey-panel.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simPod/grafana-sankey-panel/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "11.0.5",
    "@grafana/e2e-selectors": "11.2.1",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/d3": "^7.0.0",
    "@types/d3-sankey": "^0.12.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^7.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^11.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.79.3",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.2",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "11.13.0",
    "@grafana/data": "~11.2.0",
    "@grafana/runtime": "11.2.1",
    "@grafana/schema": "11.2.1",
    "@grafana/ui": "~11.2.0",
    "d3": "^7.0.0",
    "d3-sankey": "^0.12.1",
    "d3-scale-chromatic": "^3.0.0",
    "lodash": "^4.17.20",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "2.7.0"
  },
  "engines": {
    "node": "^16 || ^17 || ^18 || ^19 || ^20"
  },
  "resolutions": {
    "eslint-plugin-jsdoc": "48.11.0"
  },
  "homepage": "https://github.com/simPod/grafana-sankey-panel",
  "packageManager": "yarn@4.5.0"
}
